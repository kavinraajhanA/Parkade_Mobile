<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PageProfile.BookingDetails"
             NavigationPage.HasNavigationBar="False">
    <ContentPage.Content>
        <StackLayout>
            <StackLayout Orientation="Horizontal">
                <ImageButton Source="Vector" HeightRequest="25" WidthRequest="25" Aspect="AspectFit" BackgroundColor="Transparent" Margin="10,5,5,5" Clicked="PrevProfilePage"></ImageButton>
                <Label Text="Booking Details" FontFamily="poppins" TextColor="#223A66" HorizontalOptions="CenterAndExpand" HorizontalTextAlignment="Center" FontAttributes="Bold" FontSize="Title" Margin="0,10,20,10"/>
            </StackLayout>
           
        </StackLayout>
    </ContentPage.Content>
</ContentPage>